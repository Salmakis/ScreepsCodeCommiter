		<Label Content="shard" HorizontalAlignment="Left" Margin="10,7,0,0" VerticalAlignment="Top" Width="85"/>
		<TextBox HorizontalAlignment="Left" Height="23" Margin="75,52,0,0" TextWrapping="Wrap" Text="W56N61" VerticalAlignment="Top" Width="60" Name="InRoom"/>
		<Label Content="room" HorizontalAlignment="Left" Margin="10,49,0,0" VerticalAlignment="Top"/>
	</Grid>
</Window>
