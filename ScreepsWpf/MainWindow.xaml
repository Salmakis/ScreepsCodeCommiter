		<Grid.RowDefinitions>
			<RowDefinition Height="*"/>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>
		<TextBox Grid.Row="1" IsReadOnly="True" TextWrapping="Wrap" Text="fdsfsdfsfsdfsf" HorizontalAlignment="Stretch" Name="OutResponce"/>
